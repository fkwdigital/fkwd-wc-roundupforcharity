{
  "name": "fkwdplugins",
  "version": "1.0.0",
  "private": true,
  "description": "WordPress plugin build for FKWD",
  "author": "FKW Digital",
  "license": "GPL-2.0-or-later",
  "main": "build/index.js",
    "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "scripts": {
    "start": "gulp",
    "build": "gulp build --prod",
    "build:inline": "gulp buildInline",
    "lint": "npm run lint:css && npm run lint:js && npm run lint:php",
    "lint:css": "wp-scripts lint-style",
    "lint:js": "wp-scripts lint-js",
    "lint:php": "composer lint",
    "format": "npm run format:js && npm run format:php",
    "format:js": "wp-scripts format",
    "format:php": "prettier --write '/*.php'",
    "format:check": "prettier --check '/*.php'",
    "packages-update": "wp-scripts packages-update",
    "plugin-zip": "wp-scripts plugin-zip",
    "block:build": "wp-scripts build --webpack-copy-php && wp-scripts build-blocks-manifest",
    "block:start": "wp-scripts start --webpack-copy-php && wp-scripts build-blocks-manifest"
  },
  "devDependencies": {
    "@prettier/plugin-php": "^0.24.0",
    "@stylistic/stylelint-plugin": "^4.0.0",
    "@wordpress/eslint-plugin": "^21.5.0",
    "@wordpress/prettier-config": "^4.12.0",
    "@wordpress/scripts": "^31.0.0",
    "@wordpress/stylelint-config": "^23.8.0",
    "autoprefixer": "^10.4.20",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^7.1.2",
    "del": "^7.1.0",
    "file-loader": "^6.2.0",
    "gulp": "^5.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-if": "^3.0.0",
    "gulp-json-to-css-variables": "^1.0.2",
    "gulp-path": "^4.0.0",
    "gulp-postcss": "^10.0.0",
    "gulp-rename": "^2.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-terser": "^2.1.0",
    "html-webpack-plugin": "^5.6.3",
    "merge-stream": "^2.0.0",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.4.47",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "sass": "^1.83.1",
    "sharp": "^0.33.5",
    "style-loader": "^4.0.0",
    "stylelint-order": "^7.0.0",
    "url-loader": "^4.1.1",
    "yargs": "^17.7.2"
  },
  "dependencies": {
    "gulp-sass": "^6.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "overrides": {
    "terser": "^5.36.0",
    "@babel/runtime": "^7.26.10",
    "webpack-dev-server": "^5.2.1"
  }
}
